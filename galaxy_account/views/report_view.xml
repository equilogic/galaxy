<openerp>
	<data>
		<!-- Tax Invoice Qweb Report -->
		<report id="galaxy_tax_invoice" model="account.invoice" string="Tax Invoice"
			report_type="qweb-pdf" name="galaxy_account.report_galaxy_product_tax_invoice"
			file="galaxy_account.report_galaxy_product_tax_invoice" />


		<record id="paperformat_euro_lowmargin_galaxy" model="report.paperformat">
			<field name="name">European A4 low margin1</field>
			<field name="default" eval="True" />
			<field name="format">A4</field>
			<field name="orientation">Portrait</field>
			<field name="margin_top">55</field>
			<field name="margin_bottom">60</field>
			<field name="margin_left">10</field>
			<field name="margin_right">10</field>
			<field name="header_line" eval="False" />
			<field name="header_spacing">40</field>
			<field name="dpi">100</field>
			<field name="report_ids"
				eval="[(4, ref('galaxy_account.galaxy_tax_invoice'))]" /> 
		</record>

		<!-- Tax Invoice Qweb Report -->
		<report 
			id="galaxy_local_tax_invoice" 
			model="account.invoice" 
			string="Local Tax Invoice"
			report_type="qweb-pdf" 
			name="galaxy_account.report_galaxy_local_tax_invoice"
			file="galaxy_account.report_galaxy_local_tax_invoice" 
			/>

            
        <record id="paperformat_euro_lowmargin" model="report.paperformat">
            <field name="name">European A4 margin</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">70</field>
            <field name="margin_bottom">20</field>
            <field name="margin_left">7</field>
            <field name="margin_right">7</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">45</field>
            <field name="dpi">20</field>
            <field name="report_ids" eval="[(4, ref('galaxy_account.galaxy_local_tax_invoice'))]"/>
        </record>


	</data>
</openerp>
