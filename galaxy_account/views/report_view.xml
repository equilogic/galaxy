<openerp>
	<data>
		<!-- Tax Invoice Qweb Report -->
		<report 
			id="galaxy_tax_invoice" 
			model="account.invoice" 
			string="Tax Invoice"
			report_type="qweb-pdf" 
			name="galaxy_account.report_galaxy_product_tax_invoice"
			file="galaxy_account.report_galaxy_product_tax_invoice" />


		<record id="paperformat_euro_lowmargin_galaxy_tax" model="report.paperformat">
			<field name="name">European A4 low margin1 Tax Invoice</field>
			<field name="default" eval="True" />
			<field name="format">A4</field>
			<field name="orientation">Portrait</field>
			<field name="margin_top">55</field>
			<field name="margin_bottom">88</field>
			<field name="margin_left">10</field>
			<field name="margin_right">10</field>
			<field name="header_line" eval="False" />
			<field name="header_spacing">40</field>
			<field name="dpi">100</field>
			<field name="report_ids"
				eval="[(4, ref('galaxy_account.galaxy_tax_invoice'))]" /> 
		</record>

		<!-- Local Tax Invoice Qweb Report -->
		<report 
			id="galaxy_local_tax_invoice" 
			model="account.invoice" 
			string="Local Tax Invoice"
			report_type="qweb-pdf" 
			name="galaxy_account.report_galaxy_local_tax_invoice"
			file="galaxy_account.report_galaxy_local_tax_invoice" 
			/>

            
        <record id="paperformat_euro_lowmargin_local_tax" model="report.paperformat">
            <field name="name">European A4 margin Local tax</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">70</field>
            <field name="margin_bottom">90</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">45</field>
            <field name="dpi">20</field>
            <field name="report_ids" eval="[(4, ref('galaxy_account.galaxy_local_tax_invoice'))]"/>
        </record>


		<!-- Bank Invoice Qweb Report -->
		<report 
			id="galaxy_bank_invoice" 
			model="account.invoice" 
			string="Bank Invoice"
			report_type="qweb-pdf" 
			name="galaxy_account.report_galaxy_bank_invoice"
			file="galaxy_account.report_galaxy_bank_invoice" 
			/>

            
        <record id="paperformat_euro_lowmargin_galax_bank" model="report.paperformat">
            <field name="name">European A4 margin Bank</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">53</field>
            <field name="margin_bottom">85</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">45</field>
            <field name="dpi">20</field>
            <field name="report_ids" eval="[(4, ref('galaxy_account.galaxy_bank_invoice'))]"/>
        </record>

        <!-- Sales Register Qweb Report -->
		<report 
			id="galaxy_sales_register_pdf"
			string="Sales Register" 
			model="account.invoice"
			name="galaxy_account.report_galaxy_sales_register_pdf" 
			report_type="qweb-pdf" 
			menu="False" 
			/>

        <record id="paperformat_euro_lowmargin_galaxy_sales_register" model="report.paperformat">
            <field name="name">European A4 margin PDF</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">60</field>
            <field name="margin_bottom">20</field>
            <field name="margin_left">8</field>
            <field name="margin_right">8</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">48</field>
            <field name="dpi">80</field>
            <field name="report_ids" eval="[(4, ref('galaxy_account.galaxy_sales_register_pdf'))]"/>
        </record>

        <!-- Purchase Report Qweb Report -->
        <report 
                id="galaxy_purchase_order_template_pdf_id"
                string = "Purchase Register"
                model="account.invoice" 
                name = "galaxy_account.galaxy_purchase_order_template_pdf"
                report_type = "qweb-pdf"
                menu="False"/>

        <record id="paperformat_euro_lowmargin_galaxy_purchase_register" model="report.paperformat">
            <field name="name">European A4 margin</field>
            <field name="default" eval="True" />
            <field name="format">A4</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">60</field>
            <field name="margin_bottom">60</field>
            <field name="margin_left">10</field>
            <field name="margin_right">10</field>
            <field name="header_line" eval="False" />
            <field name="header_spacing">48</field>
            <field name="dpi">20</field>
            <field name="report_ids" eval="[(4, ref('galaxy_account.galaxy_purchase_order_template_pdf_id'))]"/>
        </record>
                
	</data>
</openerp>
